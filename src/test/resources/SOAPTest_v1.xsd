<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.quintiles.com/schemas/SOAPTest/v1"
        xmlns:tns="http://www.quintiles.com/schemas/SOAPTest/v1"
        elementFormDefault="qualified">

    <element name="SOAPTestRequest" type="tns:SOAPTestRequestType"/>
    <element name="SOAPTestResponse" type="string"/>
    <element name="SOAPTestFault" type="tns:FaultType"/>

    <complexType name="FaultType">
        <sequence>
            <element name="source" type="string"/>
            <element name="description" type="string"/>
        </sequence>
    </complexType>

    <complexType name="SOAPTestRequestType">
        <sequence>
            <element name="title" type="string"/>
            <element name="approvalDate" type="date"/>
        </sequence>
    </complexType>
</schema>